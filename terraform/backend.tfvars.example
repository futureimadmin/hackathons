# Backend configuration for Terraform state management
# Copy this file to backend.tfvars and update with your values
# Usage: terraform init -backend-config=backend.tfvars

bucket         = "ecommerce-ai-platform-terraform-state"
key            = "terraform.tfstate"
region         = "us-east-1"
encrypt        = true
dynamodb_table = "ecommerce-ai-platform-terraform-locks"
